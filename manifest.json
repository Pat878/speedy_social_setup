{
    "manifest_version": 2,
    "name": "Speedy Social Setup",
    "description": "Making branding specialists' lives easier",
    "version": "1.0",
    "homepage_url": "https://github.com/pat878",
    "browser_action": {
        "default_icon": "favicon.png",
        "default_popup": "popup.html",
        "default_title": "FormFiller"
    },
    "content_scripts": [{
        "matches": ["<all_urls>"],
        "all_frames": true,
        "js":      ["scripts/content.js"]
    }],
    "permissions": [
        "activeTab",
        "storage"
    ],
    "content_security_policy": "script-src 'self' https://www.gstatic.com/; object-src 'self'"
}
